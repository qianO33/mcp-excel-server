{
  "name": "@qian33/mcp-excel-server",
  "version": "1.0.3",
  "private": false,
  "description": "Model Excel servers",
  "author": "qianO33",
  "license": "ISC",
  "homepage": "https://www.npmjs.com/package/@qian33/mcp-excel-server",
  "bugs": "https://github.com/qianO33/mcp-excel-server/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qianO33/mcp-excel-server.git"
  },

  "main": "src/index.ts",
  "bin": {
    "mcp-excel-server": "dist/src/index.js"
  },
  "files": [
    "dist/*"
  ],
  "workspaces": [
    "src/*"
  ],
  "scripts": {
    "dev": "tsc && node dist/index.js",
    "dev:watch": "nodemon --watch 'index.ts' 'src/**/*.ts' --exec 'ts-node' src/index.ts '/Users/qian/Desktop'",
    "prepare": "npm run build",
    "watch": "tsc --watch",
    "build": "tsc",
    "publish": "npm publish --access public"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.10.2",
    "express": "^5.1.0",
    "typescript": "^5.8.3",
    "xlsx": "^0.18.5",
    "zod": "^3.24.3",
    "zod-to-json-schema": "^3.24.5"
  },
  "devDependencies": {
    "@types/express": "^5.0.1",
    "@types/node": "^20.10.5",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2"
  }
}
